    "def gen_RGG(n,p,dim):\n",
    "    D={i:np.random.uniform(0,1,dim) for i in range(n)}\n",
    "    M=np.zeros((n,n))\n",
    "    for pair in combinations(list(D.keys()),2):\n",
    "        i,j=pair\n",
    "        M[i][j]=M[j][i]=dist(D[i],D[j])\n",
    "    G=nx.from_numpy_array(M)\n",
    "    df=pd.DataFrame()\n",
    "    df[\"edge\"]=list(G.edges())\n",
    "    df[\"weight\"]=[G[e[0]][e[1]][\"weight\"] for e in G.edges()]\n",
    "    \n",
    "    df=df.sort_values(\"weight\").reset_index()\n",
    "    counter=0\n",
    "    den=0\n",
    "    comb=binom(n,2)\n",
    "    H=nx.Graph()\n",
    "    H.add_nodes_from(list(G.nodes()))\n",
    "    \n",
    "    E=(i for i in df.edge)\n",
    "    while den<=p:\n",
    "        counter+=1\n",
    "        den=counter/comb\n",
    "        try:\n",
    "            edge=next(E)\n",
    "            a,b=edge\n",
    "            H.add_edge(*edge)\n",
    "            H[a][b][\"weight\"]=G[a][b][\"weight\"]\n",
    "        except:\n",
    "            next\n",
    "        \n",
    "    return H"
   ]
